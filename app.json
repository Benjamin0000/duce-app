{
  "expo": {
    "name": "triab-app",
    "slug": "triab-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dulce_logo_2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": ["INTERNET"], 
      "package": "com.benen50.triabapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "83758034-d2d6-42a1-bb08-86839875c18f"
      }
    },
    "owner": "benen50"
  }
}
